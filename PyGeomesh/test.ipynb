      "    line: 240\n",
      "    triangle: 5218\n",
      "    tetra: 27854\n",
      "    vertex: 8\n"
     ]
    }
   ],
   "source": [
    "my_geom = MyGeom()\n",
    "my_geom.synchronize()\n",
    "mesh = my_geom.generate_mesh(3)\n",
    "print(mesh)"
