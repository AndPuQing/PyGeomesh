  {
   "cell_type": "code",
   "execution_count": 2,
   "outputs": [],
   "source": [
    "class MyGeom(pym.Geometry):\n",
    "    def __init__(self):\n",
    "        super().__init__()\n",
    "        self.add_box([0.0, 0.0, 0.0], [1, 2, 3], mesh_size=0.1)"
   ],
